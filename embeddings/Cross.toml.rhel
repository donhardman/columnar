[target.'x86_64-unknown-linux-gnu']
image = "${CROSS_IMAGE}"
pre-build = [
	"yum update -y",
	"yum groupinstall -y 'Development Tools'",
	"yum install openssl-devel -y",
]

[target.'aarch64-unknown-linux-gnu']
image = "${CROSS_IMAGE}"
pre-build = [
	"yum update -y",
	"yum groupinstall -y 'Development Tools'",
	"yum install openssl-devel -y",
]

